 {
    "builds":
    [
        {
            "src":"peady/wsgi.py",
            "use":"@vercel/python", 
            "config":
            {
                "maxLambdaSize":"50mb",
                "runtime":"python3.11"
            }                 

        }
    ],
    "buildCommand": "python manage.py collectstatic --noinput",
    "routes":
    [
        {
            "src":"/(.*)",
            "dest":"peady/wsgi.py"
        }
    ]
 }