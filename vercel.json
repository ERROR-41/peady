{
  "builds": [
    {
      "src": "peady/wsgi.py",
      "use": "@vercel/python",
       "config":
            {
                "maxLambdaSize":"100mb",
                "runtime":"python3.11"
            } 
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "peady/wsgi.py" }
  ]
}